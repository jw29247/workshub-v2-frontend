{
  "name": "workshub-frontend",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "description": "WorksHub Frontend - Modern React dashboard for agency operations",
  "homepage": "https://github.com/your-org/workshub-v2-frontend#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/workshub-v2-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/workshub-v2-frontend/issues"
  },
  "keywords": ["react", "typescript", "dashboard", "operations", "agency"],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit --project tsconfig.app.json",
    "check": "npm run lint && npm run typecheck",
    "preview": "vite preview",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:install-browsers": "npx playwright install --with-deps",
    "clean": "rm -rf dist node_modules/.cache",
    "start": "npm run dev"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react-router-dom": "^5.3.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.535.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-is": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.47.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.3.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.30.1",
    "vite": "^7.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
